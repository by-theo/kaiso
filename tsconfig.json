{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": [
      "es5",
      "dom",
      "es2015",
      "es2015.collection"
    ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "lib",
    "strict": true,
    "moduleResolution": "node",
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "lib"
  ]
}